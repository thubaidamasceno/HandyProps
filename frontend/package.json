    "build": "node scripts/build.js",
    "buildup": "node scripts/build.js && up.bat",
    "up": "up.bat",
    "over": "cp -r modules_override/* node_modules ",
    "test": "cross-env PORT=4100 node scripts/test.js --env=jsdom"
